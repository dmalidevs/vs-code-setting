{
  "editor.fontSize": 12,
  "editor.lineHeight": 20,
  "editor.fontWeight": "600",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingStrategy": "advanced",
  "editor.mouseWheelZoom": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorBlinking": "expand",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrains Mono, Consolas, Menlo, Monaco, 'Courier New', monospace",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1,
  "html.format.wrapLineLength": 1200,
  "intelephense.format.braces": "k&r",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "terminal.integrated.fontSize": 15,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "JetBrains Darcula Theme",
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#5a5f66",
    "activityBar.background": "#5a5f66",
    "sideBar.background": "#282c34",
    "terminal.background": "#282c34",
  },
  "editor.tokenColorCustomizations": {
    "[JetBrains Darcula Theme]": {
      "textMateRules": [
        {
          "scope": [
            "source.python"
          ],
          "settings": {
            "foreground": "#ba101e",
          }
        }
      ]
    }
  },
  "laravelGoto.staticFileExtensions": [
    "js",
    "ts",
    "jsx",
    "vue",
    "css",
    "scss",
    "sass",
    "less",
    "styl",
    "htm",
    "html",
    "xhtml",
    "xml",
    "log",
    "php"
  ]
}
